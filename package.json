{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "main": "index.js",
  "author": "Adao Lima <hi@adaolima.com>",
  "dependencies": {
    "babel-plugin-styled-components": "^1.10.7",
    "bulma": "^0.8.0",
    "dotenv": "^8.2.0",
    "gatsby": "^2.18.12",
    "gatsby-awesome-pagination": "^0.3.5",
    "gatsby-background-image": "^1.1.1",
    "gatsby-image": "^2.2.34",
    "gatsby-paginate": "^1.1.1",
    "gatsby-plugin-glamor": "^2.1.19",
    "gatsby-plugin-manifest": "^2.2.31",
    "gatsby-plugin-netlify": "^2.1.31",
    "gatsby-plugin-offline": "^3.0.27",
    "gatsby-plugin-purgecss": "^4.0.1",
    "gatsby-plugin-react-helmet": "^3.1.16",
    "gatsby-plugin-sass": "^2.1.27",
    "gatsby-plugin-sharp": "^2.3.5",
    "gatsby-plugin-styled-components": "^3.1.19",
    "gatsby-plugin-stylus": "^2.1.21",
    "gatsby-plugin-typography": "^2.3.21",
    "gatsby-remark-images": "^3.1.42",
    "gatsby-source-filesystem": "^2.1.40",
    "gatsby-source-wordpress": "^3.1.58",
    "gatsby-transformer-remark": "^2.6.48",
    "gatsby-transformer-sharp": "^2.3.7",
    "glamor": "^2.20.40",
    "html-react-parser": "^0.10.1",
    "lodash": "^4.17.15",
    "node-sass": "^7.0.0",
    "parcel-bundler": "^1.12.4",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-typography": "^0.16.19",
    "rupture": "^0.7.1",
    "sharp": "^0.25.2",
    "styled-components": "^5.0.1",
    "typography": "^0.16.19",
    "typography-theme-wordpress-2013": "^0.16.19",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "prettier": "^1.19.1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "NODE_ENV=production gatsby build",
    "develop:prod": "npm run clean && NODE_ENV=production gatsby develop",
    "develop": "npm run clean && NODE_ENV=development gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "prettier": {
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
